public class ascii {
	  public static void main(String[] args)
      {
           System.out.println("List of ASCII Characters are given below: ");
          
           for(int i = 65; i <=90; i++)
           {
                System.out.format("%1$-5d", i);
                System.out.format("%1$-2c", (char) i);
                System.out.println();  
           }
      }

}
